<div class="box register_box">
<div class="box_title">Register</div>
<%= form_for :user, :remote => true, url: users_path, :html => {:id => "register_form"} do |f| %>
  <div>
    <%= f.label :first_name %>
  </div>
  <div>  
    <%= f.text_field :first_name %>
  </div>
  <div>  
    <%= f.label :last_name %>
  </div>
  <div>
    <%= f.text_field :last_name %>
  </div>
  <div>
    <%= f.label :email %>
  </div>
  <div>
    <%= f.text_field :email %>
  </div>
  <div> 
    <%= f.submit "Register" %>
  </div>
<% end %>
</div>

